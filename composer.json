{
    "name": "daro/wpcore",
    "description": "Core WordPress stack via Composer",
    "authors": [
        {
            "name": "Dawid Rogaczewski",
            "homepage": "https://www.daro.de/"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "path",
            "url": "./src/plugins/intimate-tales-plugin",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./src/themes/intimate-tales-theme",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "composer",
            "url": "https://connect.advancedcustomfields.com"
        }
    ],
    "config": {
        "vendor-dir": "web/vendor",
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true,
            "composer/installers": true
        }
    },
    "require": {
        "johnpbloch/wordpress": ">=4.9",
        "rarst/fragment-cache": "^1.3",
        "rarst/update-blocker": "^1.1",
        "intimate-tales/intimate-tales-plugin": "^1.0",
        "intimate-tales/intimate-tales-theme": "^1.0"
    },
    "require-dev": {
        "rarst/laps": "^1.4.4",
        "rarst/toolbar-theme-switcher": "^1.5",
        "wpackagist-plugin/a-fresher-cache": "*",
        "wpackagist-plugin/core-control": "*",
        "wpackagist-plugin/monster-widget": "*",
        "wpackagist-plugin/theme-check": "*",
        "wpackagist-plugin/user-switching": "*",
        "wpackagist-plugin/wcm-user-language-switcher": "*"
    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web/wp"
    }
}