{
  "key": "user_profile_fields",
  "title": "User Settings",
  "active": true,
  "page_title": "User Settings",
  "menu_slug": "user-settings",
  "data_storage": "options",
  "capability": "edit_posts",
  "advanced_configuration": 1,
  "fields": [
      {
          "key": "field_registration_toggle",
          "label": "Benutzerregistrierung",
          "name": "enable_registration",
          "type": "true_false",
          "message": "Benutzerregistrierung aktivieren?"
      },
      {
          "key": "field_login_toggle",
          "label": "Benutzerlogin",
          "name": "enable_login",
          "type": "true_false",
          "message": "Benutzerlogin aktivieren?"
      },
      {
        "key": "about_you",
        "label": "About You",
        "type": "group",
        "sub_fields": [
          {
            "key": "description",
            "label": "Description",
            "type": "textarea"
          },
          {
            "key": "preferences",
            "label": "Preferences",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "security_privacy",
        "label": "Security & Privacy",
        "type": "group",
        "sub_fields": [
          {
            "key": "account_privacy",
            "label": "Account Privacy",
            "type": "select",
            "choices": {
              "public": "Public",
              "friends_only": "Friends Only",
              "private": "Private"
            }
          }
        ]
      },
      {
        "key": "account",
        "label": "Account",
        "type": "group",
        "sub_fields": [
          {
            "key": "email",
            "label": "Email",
            "type": "email"
          },
          {
            "key": "change_password",
            "label": "Change Password",
            "type": "password"
          }
        ]
      },
      {
        "key": "user_activity",
        "label": "User Activity",
        "type": "group",
        "sub_fields": [
          {
            "key": "likes",
            "label": "Likes",
            "type": "relationship",
            "post_type": ["post"]
          },
          {
            "key": "dislikes",
            "label": "Dislikes",
            "type": "relationship",
            "post_type": ["post"]
          }
        ]
      },
      {
        "key": "invitations_affiliates",
        "label": "Invitations & Affiliates",
        "type": "group",
        "sub_fields": [
          {
            "key": "invitation_code",
            "label": "Invitation Code",
            "type": "text"
          },
          {
            "key": "invited_friends",
            "label": "Invited Friends",
            "type": "repeater",
            "sub_fields": [
              {
                "key": "friend",
                "label": "Friend",
                "type": "user"
              }
            ]
          },
          {
            "key": "affiliate_link",
            "label": "Affiliate Link",
            "type": "url"
          },
          {
            "key": "affiliate_earnings",
            "label": "Affiliate Earnings",
            "type": "number"
          }
        ]
      },
      {
        "key": "membership_subscription",
        "label": "Membership or Subscription",
        "type": "group",
        "sub_fields": [
          {
            "key": "membership_status",
            "label": "Membership Status",
            "type": "select",
            "choices": {
              "free": "Free",
              "bronze": "Bronze",
              "silver": "Silver",
              "gold": "Gold"
            },
            "default_value": "free"
          },
          {
            "key": "membership_date",
            "label": "Membership Date",
            "type": "date_picker"
          },
          {
            "key": "subscription_type",
            "label": "Subscription Type",
            "type": "select",
            "choices": {
              "monthly": "Monthly",
              "yearly": "Yearly"
            }
          },
          {
            "key": "subscription_cost",
            "label": "Subscription Cost",
            "type": "number"
          },
          {
            "key": "next_payment_date",
            "label": "Next Payment Date",
            "type": "date_picker"
          }
        ]
      }
    ]
}